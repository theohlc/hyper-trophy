<h1>WorkoutsExercises#edit</h1>
<p>Find me in app/views/workouts_exercises/edit.html.erb</p>
<%= form_with model: @workouts_exercise, url: workout_workouts_exercise_path do |f| %>
    <%= f.label :exercise_name, "Exercise Name:"%>
    <%= f.text_field :exercise_name %><br>

    <%= f.label :sets, "Sets:" %>
    <%= f.number_field :sets %><br>

    <%= f.label :reps, "Reps:" %>
    <%= f.number_field :reps %><br>

    <%= f.submit "Submit"%>
<% end %>
